{
  "name": "grafana-timestream-datasource",
  "version": "1.3.1-dev",
  "description": "Load data timestream in grafana",
  "scripts": {
    "build": "yarn remove-data-node_modules && rm -rf dist && ./node_modules/.bin/grafana-toolkit grafana-toolkit plugin:build && mage build:backend",
    "test": "yarn remove-data-node_modules && ./node_modules/.bin/grafana-toolkit grafana-toolkit plugin:test",
    "dev": "rm -rf node_modules/@grafana/data/node_modules; grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "e2e": "grafana-e2e run",
    "e2e:open": "grafana-e2e open",
    "e2e:update": "grafana-e2e run --update-screenshots",
    "remove-data-node_modules": "rm -rf ./node_modules/@grafana/data/node_modules/rxjs"
  },
  "repository": "github:grafana/timestream-datasource",
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/aws-sdk": "0.0.31",
    "@grafana/data": "7.4.0",
    "@grafana/runtime": "7.4.0",
    "@grafana/toolkit": "latest",
    "@grafana/ui": "7.2.0",
    "@grafana/e2e": "latest",
    "@types/lodash": "latest"
  }
}
